{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "off",
    "no-control-regex": "off",
    "no-useless-escape": "off",
    "no-empty": "off",
    "no-undef": "off"
  },
  "globals": {
    "google": "readonly",
    "React": "readonly"
  }
}
